}

@ARTICLE{Kontogeorgos20199,
author={Kontogeorgos, A. and Chatzitheodoridis, F.},
title={Workers or investors? Investigating the reciprocity aspects among Greek social enterprises members [¿Trabajadores o Inversores? Investigar los aspectos de reciprocidad entre los miembros de las Empresas Sociales Griegas]},
journal={REVESCO Revista de Estudios Cooperativos},
year={2019},
